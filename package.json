{
  "name": "emojione-picker",
  "version": "0.2.4",
  "author": "Tom Moor",
  "description": "An emoji picker in React",
  "main": "lib/picker.js",
  "scripts": {
    "build": "babel src -d lib",
    "preview": "./node_modules/.bin/browserify -d -t [ reactify --es6 ] preview.jsx -o lib/preview.js",
    "watch": "./node_modules/.bin/watchify -d -t [ reactify --es6 ] src/*.jsx preview.jsx -o lib/preview.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tommoor/emojione-picker.git"
  },
  "bugs": {
    "url": "https://github.com/tommoor/emojione-picker/issues"
  },
  "license": "MIT",
  "peerDependencies": {
    "underscore": ">=1.8.0",
    "emojione": "^2.0.1",
    "react": ">=0.14.0"
  },
  "devDependencies": {
    "babel-preset-react": "^6.3.13",
    "browserify": "^10.0.0",
    "emojione": "^2.0.1",
    "react": "~0.14.0-rc1",
    "react-dom": "~0.14.0-rc1",
    "react-tools": "~0.12.0",
    "reactify": "git://github.com/andreypopp/reactify.git#c1b0e15eca9d5e663f34f372682723d5fb4f15af",
    "underscore": "^1.8.3",
    "watchify": "^3.2.2"
  }
